unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_1_10000.jpg"
set title "Timeline View of Trimmed Data -- 10000 Client-Pairs -- One Router"
set arrow from 0,7.199049 to 100,7.199049 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points lc rgb "blue"

