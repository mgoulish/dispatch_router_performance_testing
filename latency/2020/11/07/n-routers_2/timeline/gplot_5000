unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_5000.jpg"
set title "Timeline View of Trimmed Data -- 5000 Client-Pairs -- Two Routers"
set arrow from 0,1.413107 to 100,1.413107 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points

