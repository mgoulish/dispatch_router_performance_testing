unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_1_8000.jpg"
set title "Timeline View of Trimmed Data -- 8000 Client-Pairs -- One Router"
set arrow from 0,1.409054 to 100,1.409054 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points lc rgb "blue"

