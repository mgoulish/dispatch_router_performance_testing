unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_4000.jpg"
set title "Timeline View of Trimmed Data -- 4000 Client-Pairs -- Two Routers"
set arrow from 0,1.144886 to 100,1.144886 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points

