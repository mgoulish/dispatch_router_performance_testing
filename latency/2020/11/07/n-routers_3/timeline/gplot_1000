unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_3_1000.jpg"
set title "Timeline View of Trimmed Data -- 1000 Client-Pairs -- Three Routers"
set arrow from 0,0.638 to 100,0.638 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points

