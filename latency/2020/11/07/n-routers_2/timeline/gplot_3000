set autoscale
unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_3000.jpg"
set title "Timeline View of Trimmed Data -- 3000 Client-Pairs -- Two Routers"
set arrow from 0,0.941992 to 100,0.941992 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points

