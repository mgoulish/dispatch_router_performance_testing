unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_3_3000.jpg"
set title "Timeline View of Trimmed Data -- 3000 Client-Pairs -- Three Routers"
set arrow from 0,1.56 to 100,1.56 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points

