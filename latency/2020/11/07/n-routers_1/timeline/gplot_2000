set autoscale
unset key
set ylabel "latency"
set xlabel "time"
set terminal jpeg size 2000,500
set output "timeline_1_2000.jpg"
set title "Timeline View of Trimmed Data -- 2000 Client-Pairs -- One Router"
set arrow from 0,0.185013 to 100,0.185013 nohead front lc rgb "red" lw 4
plot "trimmed_data" with points lc rgb "blue"

